{
  "name": "moment-business-days-sk",
  "version": "1.0.0",
  "description": "MomentJS plugin to use business days with slovak holidays",
  "repository": {
    "type": "git",
    "url": "https://github.com/mirobartanus/moment-business-days-sk.git"
  },
  "keywords": [
    "business",
    "days",
    "moment",
    "momentjs",
    "plugin",
    "slovak"
  ],
  "author": "Miro Bartanus <miroslav.bartanus@gmail.com>",
  "homepage": "https://github.com/mirobartanus/moment-business-days-sk",
  "license": "MIT",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "start": "node ./dist/index.js",
    "test": "mocha --slow 0 -r ts-node/register test/**/*.spec.ts",
    "test:args": "mocha --slow 0 -r ts-node/register",
    "clean": "del-cli ./dist",
    "copyfiles": "copyfiles CHANGELOG.md LICENSE package.json package-lock.json README.md dist",
    "build": "tsc",
    "build:prod": "tsc -p tsconfig-prod.json",
    "rebuild": "npm run clean && npm run build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.spec.ts\" ",
    "lint": "tslint -p tsconfig.json",
    "prepack": "npm run clean && npm run build:prod",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "devDependencies": {
    "@types/chai": "^4.2.4",
    "@types/mocha": "^5.2.7",
    "chai": "^4.2.0",
    "copyfiles": "^2.1.1",
    "del-cli": "^3.0.0",
    "mocha": "^6.2.2",
    "prettier": "^1.18.2",
    "ts-node": "^8.4.1",
    "tslint": "^5.20.1",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.7.2"
  },
  "dependencies": {
    "moment": "^2.24.0",
    "moment-business-days": "^1.1.3"
  }
}
